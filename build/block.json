  "apiVersion": 3,
  "name": "alibitech/vendy",
  "version": "1.0.0",
  "title": "Vendy",
  "category": "widgets",
  "description": "Embed a Vendy layout.",
  "textdomain": "alibitech-vendy",
  "editorScript": "file:./index.js",
  "render": "file:./render.php",
