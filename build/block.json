  "style": "file:./style-index.css",
  "viewScript": "file:./widget-embed.js",
  "attributes": {
    "layoutId": {
      "type": "string"
    },
    "hostUrl": {
      "type": "string",
      "default": "https://app.shopvendy.ca"
    }
  }
}